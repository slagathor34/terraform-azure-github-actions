# Example Terraform variables file
# Copy this file to terraform.tfvars and customize the values

project_name = "myproject"
environment = "dev"
location = "East US"

# Optional: Specify a custom resource group name
# resource_group_name = "my-custom-rg-name"

# Custom tags
tags = {
  Environment = "dev"
  Project     = "my-terraform-project"
  ManagedBy   = "terraform"
  Owner       = "devops-team"
}
